{
    "name": "vmorozov/laravel_admin_generator",
    "type": "library",
    "description": "This package is made to speed up development of admin panel for your laravel project",
    "keywords": ["laravel", "admin panel generation", "administration", "generators"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Morozov",
            "email": "freezer278@gmail.com",
            "homepage": "https://github.com/freezer278",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "almasaeed2010/adminlte": "2.4.*",
        "illuminate/database": ">=5.3",
        "illuminate/http": ">=5.3",
        "illuminate/routing": ">=5.3",
        "illuminate/support": ">=5.3",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-medialibrary": ">=7.1",
        "vmorozov/laravel-file-uploads": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.*",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Vmorozov\\LaravelAdminGenerator\\": "src",
            "Vmorozov\\LaravelAdminGenerator\\App\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vmorozov\\LaravelAdminGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vmorozov\\LaravelAdminGenerator\\AdminGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
